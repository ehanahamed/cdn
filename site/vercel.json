{
  "rewrites": [
    {
      "source": "/:repo/:path*",
      "destination": "https://raw.githubusercontent.com/ehanahamed/:repo/main/:path*"
    }
  ]
  "headers": [
    {
      "source": "*.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]
}
